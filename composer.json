{
  "name": "alcodo/powerimage",
  "description": "Laravel Package to create a dynamic image handler",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "glide",
    "http",
    "image",
    "dynamic",
    "laravel"
  ],
  "homepage": "https://github.com/alcodo/powerimage",
  "authors": [
    {
      "name": "Alexej Kloos",
      "email": "alexejkloos@gmail.com",
      "homepage": "http://alcodo.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "5.1.*|5.2.*",
    "league/glide": "^1.0",
    "approached/laravel-image-optimizer": "^2.0",
    "cocur/slugify": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Alcodo\\PowerImage\\": "src/"
    }
  },
  "autoload-dev": {
