{
  "name": "alcodo/powerimage",
  "description": "Laravel Package to create a dynamic image handler",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "glide",
    "http",
    "image",
    "dynamic",
    "laravel"
  ],
  "homepage": "https://github.com/alcodo/powerimage",
  "authors": [
    {
      "name": "Alexej Kloos",
      "email": "alexejkloos@gmail.com",
      "homepage": "https://alcodo.com"
    }
  ],
  "require": {
    "php" : ">=7.0",
    "illuminate/support": "~5.5.0|~5.6.0",
    "league/glide": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit" : "^5.7|6.2|^7.0",
    "orchestra/testbench": "^3.5.0|^3.6.0"
  },
  "autoload": {
    "psr-4": {
      "Alcodo\\PowerImage\\": "src/"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Alcodo\\PowerImage\\PowerImageServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
