{
  "name": "alcodo/powerimage",
  "description": "Laravel Package to create a dynamic image handler",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "glide",
    "http",
    "image",
    "dynamic",
    "laravel"
  ],
  "homepage": "https://github.com/alcodo/powerimage",
  "authors": [
    {
      "name": "Alexej Kloos",
      "email": "alexejkloos@gmail.com",
      "homepage": "http://alcodo.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "~5.6",
    "league/glide": "^1.3",
    "cocur/slugify": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Alcodo\\PowerImage\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Alcodo\\PowerImage\\PowerImageServiceProvider"
      ]
    },
    "minimum-stability": "dev"
}
